{
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/mock",
            "destination": "/mock/index.html",
            "permanent": true
        },
        {
            "source": "/mock/",
            "destination": "/mock/index.html",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/mock/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/mock/Questions.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, stale-while-revalidate=604800"
                }
            ]
        }
    ]
}